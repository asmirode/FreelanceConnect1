# Copy this file to `.env` in the same folder and fill in real values before starting the server.
# Example local MongoDB URI (no auth):
MONGO=mongodb://localhost:27017/freelanceconnect

# Example for a hosted MongoDB Atlas (replace user/password/host/db):
# MONGO=mongodb+srv://<user>:<password>@cluster0.mongodb.net/<dbname>?retryWrites=true&w=majority

# A secret string used to sign JWTs
JWT_KEY=replace_this_with_a_secure_random_string

# Other optional vars
NODE_ENV=development
PORT=8000
